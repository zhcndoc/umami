{
  "name": "umami-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "rimraf .source && next dev --turbo",
    "start": "rimraf .source && next start",
    "postinstall": "fumadocs-mdx",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check ."
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css}": [
      "npm run format"
    ]
  },
  "dependencies": {
    "fumadocs-core": "16.0.7",
    "fumadocs-mdx": "13.0.5",
    "fumadocs-ui": "16.0.7",
    "lucide-react": "^0.548.0",
    "next": "16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.3",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.9.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3"
  }
}
